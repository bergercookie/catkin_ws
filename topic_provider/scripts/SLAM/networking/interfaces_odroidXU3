auto wlan6
iface wlan6 inet static
    wireless-essid multi-robot-exp
    wireless-mode ad-hoc
    wireless-channel 5
    wireless-enc off
    address 192.168.100.17
    netmask 255.255.255.0
    broadcast 192.168.100.255
    gateway 192.168.100.1
    dns-nameservers 192.168.100.1
